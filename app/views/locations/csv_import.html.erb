<%= form_for :dump, :url=>{:controller=>"locations", :action=>"csv_import"}, :html => { :multipart => true } do |f| %>
  <div>
    Select a CSV File :
      <%= f.file_field :csv_file %>
      <%= submit_tag 'Submit' %>
    <% end %>
  </div>
  <% if @failed_rows && @failed_rows.count > 0 %>
  <ul>
    <% @failed_rows.each do |row| %>
      <li><%= row %></li>
    <% end %>
  </ul>
  <% end %>
  
  <%= link_to "Back", locations_path %>
