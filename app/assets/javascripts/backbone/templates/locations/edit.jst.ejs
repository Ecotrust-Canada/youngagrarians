<h1>Edit location</h1>

<form id="edit-location" name="location">
  <div class="field">
    <label for="latitude"> latitude:</label>
    <input type="text" name="latitude" id="latitude" value="<%= latitude %>" >
  </div>

  <div class="field">
    <label for="longitude"> longitude:</label>
    <input type="text" name="longitude" id="longitude" value="<%= longitude %>" >
  </div>

  <div class="field">
    <label for="gmaps"> gmaps:</label>
    <input type="text" name="gmaps" id="gmaps" value="<%= gmaps %>" >
  </div>

  <div class="field">
    <label for="address"> address:</label>
    <input type="text" name="address" id="address" value="<%= address %>" >
  </div>

  <div class="field">
    <label for="name"> name:</label>
    <input type="text" name="name" id="name" value="<%= name %>" >
  </div>

  <div class="field">
    <label for="content"> content:</label>
    <input type="text" name="content" id="content" value="<%= content %>" >
  </div>

  <div class="field">
    <label for="type"> type:</label>
    <input type="text" name="type" id="type" value="<%= type %>" >
  </div>

  <div class="actions">
    <input type="submit" value="Update Location" />
  </div>

</form>

<a href="#/index">Back</a>