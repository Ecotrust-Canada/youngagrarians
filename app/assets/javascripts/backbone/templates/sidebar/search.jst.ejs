<form id="search-form" method="POST" >
	<div class="search-field-container">
  	<input type="text" placeholder="Search" name="search" id="search" />
  	<a id="map-search-clear"><i class="icon-remove"></i></a>
  	<span class="alert alert-warning hide">You need to enter some search terms!</span>
	</div>
	<div>
	  <button id="search-button" class="small round button" type="button" id="start-search"><i class='icon-search'></i></button>
	</div>
</form>
