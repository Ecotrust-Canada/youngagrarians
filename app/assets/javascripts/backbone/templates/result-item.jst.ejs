<div class="row result-header">
  <h2>
      <img src="<%= img %>" />
  		<% if ( !_.isEmpty( url ) ) { %>
  		<% if ( _.isNull( url.match( 'http://' ) ) ) { url = 'http://' + url } %>
  		<a href="<%= url %>" target="_blank"><%= name %></a>
  		<% } else { %>
  		<%= name %>
  		<% }%>
  </h2>
</div>
<p class="location-description"><%= description %></p>
